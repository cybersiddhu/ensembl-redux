{
  "name": "dicty-redux",
  "version": "0.0.1",
  "description": "showcase of redux using ensembl api",
  "main": "index.js",
  "scripts": {
    "clean": "rm -rf dist",
    "compile": "webpack && npm run compile:cleanup",
    "compile:cleanup": "rm dist/client/index.html",
    "dev": "node --harmony ./config/webpack-dev-server",
    "dev:debug": "npm run dev -- --debug",
    "dev:quiet": "npm run dev -- --quiet",
    "test": "npm run test:unit",
    "test:unit": "./node_modules/karma/bin/karma start",
    "deploy": "npm run test && npm run compile",
    "server:start": "node --harmony ./server/scripts/start"
  },
  "author": "Siddhartha Basu <biosidd@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "babel-core": "5.x",
    "babel-eslint": "3.x",
    "babel-loader": "5.x",
    "babel-runtime": "5.x",
    "file-loader": "~0.8.1",
    "core-js": "~1.0.0",
    "eslint": "0.24.x",
    "eslint-plugin-react": "3.x",
    "eslint-loader": "0.14.x",
    "html-webpack-plugin": "^1.3.0",
    "immutable": "^3.7.4",
    "object-assign": "^3.0.0",
    "react": "~0.13.3",
    "redux": "^1.0.0-rc",
    "react-redux": "0.5.x",
    "redux-thunk": "0.1.x",
    "react-router": "1.0.0-beta1",
    "radium": "~0.13.2",
    "webpack": "~1.9.7",
    "yargs": "^3.16.1",
    "axios": "~0.5.4"
  },
  "devDependencies": {
    "jasmine-core": "~2.3.4",
    "karma": "0.12.x",
    "karma-cli": "~0.0.4",
    "karma-jasmine": "~0.3.5",
    "karma-coverage": "^0.3.1",
    "karma-phantomjs-launcher": "~0.1.4",
    "karma-spec-reporter": "0.0.19",
    "karma-nyan-reporter": "~0.2.1",
    "karma-clear-screen-reporter": "~0.0.1",
    "karma-webpack": "~1.6.0",
    "karma-sourcemap-loader": "~0.3.5",
    "phantomjs-polyfill": "0.0.1",
    "react-hot-loader": "~1.2.7",
    "redux-devtools": "~0.1.2",
    "webpack-dev-server": "^1.8.2"
  }
